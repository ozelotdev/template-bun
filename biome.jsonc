{
	"$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"defaultBranch": "main",
		"useIgnoreFile": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"all": true
		}
	},
	"formatter": {
		"enabled": true
	},
	"organizeImports": {
		"enabled": true
	}
}
